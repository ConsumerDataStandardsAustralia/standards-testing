{
  "name": "@cds-au/testdocs",
  "version": "0.0.16",
  "description": "Tools for managing the presentation of test case documentation for the Australian Consumer Data Right regime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "tslint -p .",
    "sync-schema": "rm -f ./src/schema/* && cp ../../schema/cdr-test-doc-schema.json ./src/schema && node ./utils/create-schema-definitions.js",
    "build": "rm -rf ./dist/ && tsc && chmod +x ./dist/cli.js && npm run copy-additional",
    "copy-additional": "cp ./config.json ./dist/commands",
    "publish": "npm publish --access public"
  },
  "author": "Data Standards Body",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.26",
    "@types/yargs": "^17.0.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "ajv": "^8.9.0",
    "ajv-formats": "^2.1.1",
    "better-ajv-errors": "^1.1.2",
    "csv-parse": "^5.0.4",
    "dayjs": "^1.11.2",
    "json-schema-to-typescript": "^10.1.5",
    "yargs": "^17.4.1"
  }
}
