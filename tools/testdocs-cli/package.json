{
  "name": "@cds-au/testdocs-cli",
  "version": "0.0.7",
  "description": "Tools for managing the presentation of test case documentation for the Australian Consumer Data Right regime",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p .",
    "sync-schema": "rm -f ./src/schema/* && cp ../../schema/*.json ./src/schema && node ./utils/create-schema-definitions.js",
    "build": "rm -rf ./build/ && tsc && chmod +x ./build/cli.js"
  },
  "author": "Data Standards Body",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.26",
    "@types/yargs": "^17.0.8",
    "typescript": "^4.5.5"
  },
  "bin": "build/cli.js",
  "dependencies": {
    "ajv": "^8.9.0",
    "ajv-formats": "^2.1.1",
    "better-ajv-errors": "^1.1.2",
    "csv-parse": "^5.0.4",
    "json-schema-to-typescript": "^10.1.5",
    "yargs": "^17.3.1"
  }
}
